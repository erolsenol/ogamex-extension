{
  "name": "TurnBt",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "scripting", "webNavigation", "activeTab", "background", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": ["https://turn-games.com/*"],
      "all_frames": true,
      "js": ["content-script.js", "ps.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["test1.png", "test2.png"],
      "matches": ["https://www.epey.com/*"]
    },
    {
      "resources": ["test1.png", "test2.png"],
      "matches": ["https://web-accessible-resources-1.glitch.me/*"]
    },
    {
      "resources": ["test3.png", "test4.png"],
      "matches": ["https://web-accessible-resources-2.glitch.me/*"],
      "use_dynamic_url": true
    }
  ]
}
